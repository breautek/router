{
  "name": "@breautek/router",
  "version": "1.0.2",
  "description": "An alternate react router.",
  "main": "dist/router.js",
  "types": "dist/src/api.d.ts",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx '?(src|spec)/**/*.?(ts|tsx)' --cache",
    "build": "rollup -c rollup.config.js",
    "publish-beta": "npm publish --tag beta",
    "prepublishOnly": "npm run build",
    "changelog": "auto-changelog --output CHANGELOG.md -p --release-summary --ignore-commit-pattern chore",
    "docs": "typedoc --excludePrivate --includeVersion -readme ./README.md --out ./docs ./src",
    "version": "npm run-script build && npm run-script docs && npm run changelog && git add CHANGELOG.md docs && git commit -m 'chore: changelog'",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breautek/router"
  },
  "keywords": [
    "react",
    "router",
    "page",
    "screen",
    "navigation"
  ],
  "author": "norman@normanbreau.com",
  "license": "MIT",
  "homepage": "https://github.com/breautek/router",
  "peerDependencies": {
    "react": ">=16.9.0 <17.0.0",
    "react-dom": ">=16.9.0 <17.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@babel/register": "7.14.5",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@totalpave/eslint-plugin": "4.0.0",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.24",
    "@types/react": "16.14.4",
    "@types/react-dom": "16.9.11",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "ajv": "8.6.1",
    "auto-changelog": "2.3.0",
    "babel-jest": "26.6.3",
    "core-js": "3.15.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.30.0",
    "eslint-plugin-react": "7.24.0",
    "glob": "7.1.7",
    "ignore-styles": "5.0.1",
    "jest": "26.6.3",
    "node-sass": "6.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "regenerator-runtime": "0.13.7",
    "rollup": "2.53.0",
    "rollup-plugin-progress": "1.1.2",
    "rollup-plugin-sass": "1.2.2",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-node": "10.0.0",
    "typedoc": "0.21.2",
    "typedoc-plugin-markdown": "3.10.2",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@totalpave/interfaces": "1.0.0"
  }
}
